<?xml version="1.0" encoding="utf-8" ?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2" >
  <Program name="remGetMaxDist" access="private" >
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1" >
      <Parameter name="sReturn" type="string" use="reference" xsi:type="array" dimensions="1" />
    </Parameters>
    <Locals>
      <Local name="sCmd" type="string" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  // Return max dist in a single move step for remote moves (mm)
  //
  logMsg("call remGetMaxDist")
  sCmd="GETMAXDIST"
  call remGetnGlobal(nMaxAccel,sCmd,sReturn)
end]]></Code>
  </Program>
</Programs>